{
  "name": "sf-academy",
  "version": "1.0.0",
  "description": "Candidacy to Soluzioni Futura",
  "main": "index.js",
  "scripts": {
    "test:prod": "NODE_ENV=production npx ts-node \"test/pretest\" && NODE_ENV=production mocha -r ts-node/register \"test/**\"",
    "test:dev": "npm run db:reseed && mocha -r ts-node/register \"test/**\"",
    "test": "if [ \"$NODE_ENV\" != \"production\" ]; then npm run test:dev; else npm run test:prod; fi",
    "start:api:local": "npm_scripts/start-api-local.sh",
    "start:exchange:local": "npm_scripts/start-exchange-local.sh",
    "start:users:local": "npm_scripts/start-users-local.sh",
    "compile-scripts": "tsc -p postgres/tsconfig.json",
    "db:destroy": "npx ts-node \"postgres/dbExecute\" destroy",
    "db:create": "npx ts-node \"postgres/dbExecute\" create",
    "db:seed": "npx ts-node \"postgres/dbExecute\" seed",
    "db:reseed": "npx ts-node \"postgres/dbExecute\" reseed",
    "start:prod": "docker-compose build && docker-compose up && docker-compose down",
    "start:dev": "npm install && npx concurrently \"npm run start:api:local\" \"npm run start:exchange:local\" \"npm run start:users:local\"",
    "start": "if [ \"$NODE_ENV\" != \"production\" ]; then npm run start:dev; else npm run start:prod; fi"
  },
  "author": "Fabio Gulmini",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.6.0",
    "@grpc/proto-loader": "^0.6.9",
    "@protobuf-ts/plugin": "^2.4.0",
    "@types/express": "^4.17.13",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/xml2js": "^0.4.9",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-openapi": "^10.1.0",
    "express-openapi-validator": "^4.13.7",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.7",
    "openapi-typescript": "^5.2.0",
    "openapi-typescript-codegen": "^0.20.1",
    "pg": "^8.7.3",
    "typescript": "^4.6.3",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/mocha": "^9.1.0",
    "axios": "^0.26.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "concurrently": "^7.1.0",
    "mocha": "^9.2.2",
    "node-ts": "^5.1.2",
    "superagent": "^7.1.2",
    "ts-node": "^10.7.0"
  }
}
