{
  "name": "sf-academy",
  "version": "1.0.0",
  "description": "Candidacy to Soluzioni Futura",
  "main": "index.js",
  "scripts": {
    "test": "if [ \"$NODE_ENV\" != \"production\" ]; then npm run test:dev; else npm run test:prod; fi",
    "test:dev": "npm run db:reseed && mocha -r ts-node/register \"test/**\"",
    "test:prod": "npx ts-node \"test/pretest\" && mocha -r ts-node/register \"test/**\"",
    "prestart": "npm i --include=dev",
    "start": "if [ \"$NODE_ENV\" != \"production\" ]; then npm run start:dev; else npm run start:prod; fi",
    "start:prod": "npm run build-services && docker-compose build && docker-compose up && docker-compose down",
    "start:dev": "npx concurrently \"npm run start:api\" \"npm run start:exchange\" \"npm run start:users\"",
    "start:api": "npm_scripts/start-api.sh",
    "start:exchange": "npm_scripts/start-exchange.sh",
    "start:users": "npm_scripts/start-users.sh",
    "db:destroy": "npx ts-node \"postgres/dbExecute\" destroy",
    "db:create": "npx ts-node \"postgres/dbExecute\" create",
    "db:seed": "npx ts-node \"postgres/dbExecute\" seed",
    "db:reseed": "npx ts-node \"postgres/dbExecute\" reseed",
    "build-services": "for file in npm_scripts/build-* ; do chmod +x $file && $file; done"
  },
  "author": "Fabio Gulmini",
  "license": "ISC",
  "dependencies": {
    "@grpc/grpc-js": "^1.6.5",
    "@grpc/proto-loader": "^0.6.9",
    "@protobuf-ts/plugin": "^2.4.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "express-openapi": "^10.1.0",
    "express-openapi-validator": "^4.13.7",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "knex": "^1.0.7",
    "openapi-typescript": "^5.2.0",
    "openapi-typescript-codegen": "^0.20.1",
    "pg": "^8.7.3",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router": "^6.3.0",
    "react-router-dom": "^6.3.0",
    "typescript": "^4.6.3",
    "webpack": "^5.72.0",
    "xml2js": "^0.4.23"
  },
  "devDependencies": {
    "@types/chai": "^4.3.1",
    "@types/express": "^4.17.13",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/react": "^18.0.5",
    "@types/react-dom": "^18.0.1",
    "@types/xml2js": "^0.4.9",
    "axios": "^0.26.1",
    "chai": "^4.3.6",
    "chai-http": "^4.3.0",
    "concurrently": "^7.1.0",
    "mocha": "^9.2.2",
    "superagent": "^7.1.2",
    "ts-node": "^10.7.0"
  }
}
